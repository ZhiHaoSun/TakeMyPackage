<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.team7.sports">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.team7.TakeMyPackage.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.team7.TakeMyPackage.StartActivity" />
        <activity android:name="org.team7.TakeMyPackage.RegisterActivity" />
        <activity android:name="org.team7.TakeMyPackage.LoginActivity" />
        <activity android:name="org.team7.TakeMyPackage.ViewAssignmentActivity" />
        <activity android:name="org.team7.TakeMyPackage.CreateRequestActivity" />
        <activity android:name="org.team7.TakeMyPackage.RequestDetailActivity" />
    </application>

</manifest>